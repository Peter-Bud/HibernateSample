<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="entity.Country">
        <table name="country" schema="world" catalog=""/>
        <attributes>
            <id name="code">
                <column name="Code" length="3"/>
            </id>
            <basic name="name">
                <column name="Name" length="52"/>
            </basic>
            <basic name="continent">
                <column name="Continent" precision="-1"/>
            </basic>
            <basic name="region">
                <column name="Region" length="26"/>
            </basic>
            <basic name="surfaceArea">
                <column name="SurfaceArea" precision="2"/>
            </basic>
            <basic name="indepYear">
                <column name="IndepYear" nullable="false" precision="-1"/>
            </basic>
            <basic name="population">
                <column name="Population" precision="-1"/>
            </basic>
            <basic name="lifeExpectancy">
                <column name="LifeExpectancy" nullable="false" precision="1"/>
            </basic>
            <basic name="gnp">
                <column name="GNP" nullable="false" precision="2"/>
            </basic>
            <basic name="gnpOld">
                <column name="GNPOld" nullable="false" precision="2"/>
            </basic>
            <basic name="localName">
                <column name="LocalName" length="45"/>
            </basic>
            <basic name="governmentForm">
                <column name="GovernmentForm" length="45"/>
            </basic>
            <basic name="headOfState">
                <column name="HeadOfState" nullable="false" length="60"/>
            </basic>
            <basic name="capital">
                <column name="Capital" nullable="false" precision="-1"/>
            </basic>
            <basic name="code2">
                <column name="Code2" length="2"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
